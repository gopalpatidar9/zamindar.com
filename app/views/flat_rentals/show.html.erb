<p style="color: green"><%= notice %></p>

<%= render @flat_rental %>
<% @flat_rental.images.each do |image| %>
  <%= image_tag image, class: 'img-fluid', style: 'height:300px;' %>
<% end %>
<!-- Add this link/button where you want users to initiate a new message -->
<div>
  <%= link_to "Edit this flat rental", edit_flat_rental_path(@flat_rental) %> |
  <%= link_to "Back to flat rentals", flat_rentals_path %>

  <%= button_to "Destroy this flat rental", @flat_rental, method: :delete %>
</div>


